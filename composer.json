{
    "name": "eleven59/backpack-settings-extended",
    "description": "Extends Backpack settings package to allow for categorization of settings, file/image uploads, custom entity names, and custom sort order",
    "license": "MIT",
    "authors": [
        {
            "name": "Nik Linders",
            "email": "info@eleven59.nl",
            "homepage": "https://eleven59.nl"
        }
    ],
    "homepage": "https://github.com/eleven59/backpack-settings-extended",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "BackpackSettingsExtended"
    ],
    "require": {
        "illuminate/support": "~7|~8|~9",
        "backpack/crud": "^4.1.0|~5",
        "backpack/settings": "^3.0",
        "eleven59/backpack-image-traits": "^1.0",
        "mews/purifier": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Eleven59\\BackpackSettingsExtended\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eleven59\\BackpackSettingsExtended\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eleven59\\BackpackSettingsExtended\\AddonServiceProvider"
            ]
        }
    }
}
